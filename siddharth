class india():
    def capital(self):
        print("New delhi is the capital of india.")
    def language(self):
        print("hindi is the most widely speaken language.")
    def type(self):
        print("india is the developing country.")
        
class usa():
    def capital(self):
        print("washington dc is the capital of usa.")
    def capital(self):
        print("english is the primary language of us.")
    def type(self):
        print("usa is a developed country.")
        
obj_ind=india()
obj_usa=usa()
for country in(obj_ind,obj_usa):
    country.capital()
    country.language()
    country.type()
